<tr>
  <td class="list"><br/> 
  	<span class="content">
  		<%=link_to list.title, list_tasks_path(list)%>  		
  	</span><br/>
    <span class="timestamp">
      Posted <%= time_ago_in_words(list.created_at) %> ago.
    </span>
  </td>
  <% if current_user?(list.user) %>
  <td>
    <%= link_to del,list_path(list),:confirm => "Вы уверены?",:method=>:delete %>
  </td>
  <% end %>
</tr>